#to set up environment variables for gophie(replace gophie.env with path to gophie .env file) :
kubectl create secret generic gophie-secret --from-env-file=gophie.env -n gophie 
#to set up environment variables for ocena(replace gophie.env with path to ocena .env file) :
kubectl create secret generic ocena-secret --from-env-file=ocena.env -n gophie 